{
    "name": "moustacheful/default_project_slim",
    "description": "A Slim + redbean base for APIs and simple apps",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "redbeanphp",
                "version": "master",
                "dist": {
                    "url": "http://www.redbeanphp.com/downloadredbean.php",
                    "type": "tar"
                }
            }
        }

    ],
    "require": {
        "ext-mbstring": "*",
        "redbeanphp": "master@dev",
        "slim/slim": "~2.4",
        "facebook/php-sdk-v4": "~4.0",
        "anahkiasen/underscore-php": "^2.0"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^1.1"
    },
    "autoload": {
        "files": [
            "vendor/redbeanphp/rb.php"
        ],
        "classmap": [
            "app/model",
            "app/middleware",
            "app/controller"
        ]
    },
    "authors": [
        {
            "name": "moustacheful",
            "email": "suchious@gmail.com"
        }
    ]
}
